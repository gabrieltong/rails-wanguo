<h1>Listing annexes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Law</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @annexes.each do |annex| %>
  <tr>
    <td><%= annex.title %></td>
    <td><%= annex.law_id %></td>
    <td><%= annex.state %></td>
    <td><%= link_to 'Show', annex %></td>
    <td><%= link_to 'Edit', edit_annex_path(annex) %></td>
    <td><%= link_to 'Destroy', annex, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

